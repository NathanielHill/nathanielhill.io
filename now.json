{
  "name": "nathanielhill",
  "alias": "www.nathanielhill.io",
  "version": 2,
  "regions": [ "all" ],
  "builds": [
    { "src": "public/**", "use": "@now/static" },
    { "src": "www/package.json", "use": "@now/static-build" }
  ],
  "routes": [
    { "src": "^/browserconfig.xml$", "dest": "/public/browserconfig.xml" },
    { "src": "^/favicon.ico$", "dest": "/public/favicon.ico" },
    { "src": "^/manifest.json$", "dest": "/public/manifest.json" },
    { "src": "^/robots.txt$", "dest": "/public/robots.txt" },
    { "src": "^/(.*)icon(.*)\\.png", "dest": "/public/icons/$1icon$2.png"},
    { "src": "^/(.*)", "dest": "/www/$1" }
  ]
}
