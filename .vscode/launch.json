  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Server",
      "protocol": "inspector",
      "port": 9229,
      "restart": true,
      "skipFiles": [
        "${workspaceFolder}/node_modules/**/*.js"
      ]
    },
    // {
    //   "type": "node",
    //   "request": "launch",
    //   "name": "Server",
    //   "runtimeExecutable": "npm",
    //   "runtimeArgs": [
    //     "run-script",
    //     "debug"
    //   ],
    //   "skipFiles": [
    //     "${workspaceFolder}/node_modules/**/*.js"
    //   ],
    //   "port": 9229
    // },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Client",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    }
  ],
  "compounds": [
    {
      "name": "DEBUG",
      "configurations": [
        "Server",
        "Client"
      ]
    }
  ]
}